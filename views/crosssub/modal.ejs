<!-- Modal -->
<div class="modal fade" id="subcribeModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header bg-info">
        <h5 class="modal-title" id="exampleModalLabel"><span class="text-danger"><i class="fa fa-youtube-play" aria-hidden="true"></i></span> Đăng ký kênh</h5>
        <label for="" id="waittime"></label>
        <button type="submit" class="btn btn-info" id="close-modal">

          <i class="fa fa-times text-danger" aria-hidden="true"></i>
        </button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-sm-12">
            <div id="player"></div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <div class="container">
          <div class="row">
            <div class="col-sm-10">
              <h3 class="font-weight-bold text-info" id="waitdiv">Thời gian chờ: <span id="clockdiv" class="text-danger"></span></h3>
            </div>
            <div class="col-sm-2">
              <form class="" action="/crosssub/wait" method="post">
                <input type="text" name="await" id="await" hidden>
                <input type="text" name="channelId" id="channelId" hidden>
                <button type="submit" name="button" id="subscribe" class="btn btn-danger">Subscribe</button>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<style media="screen">
  #clockdiv{
    font-family:  Times, serif;
  }
</style>
